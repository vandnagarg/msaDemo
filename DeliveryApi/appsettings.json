{
  "spring": {
    "application": {
      "name": "deliveryApi"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka/",
      "ValidateCertificates": false
    },
    "instance": {
      "hostName": "localhost",
      "port": "9909",
      "instanceId": "deliveryApi1"
    }
  },
  "JWTAuthentication": {
    "SecurityKey": "This_is_the_security_Key_for_JWT_Authentication"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQHostName": "localhost"

}
