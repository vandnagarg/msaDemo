{
  "JWTAuthentication": {
    "SecurityKey": "This_is_the_security_Key_for_JWT_Authentication",
    "ValidAudience": "api_gateway",
    "ValidIssuer": "authApi"
  },
  "spring": {
    "application": {
      "name": "authApi"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka/",
      "ValidateCertificates": false
    },
    "instance": {
      "hostName": "localhost",
      "port": "44320",
      "instanceId": "authApi1"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*"
}
